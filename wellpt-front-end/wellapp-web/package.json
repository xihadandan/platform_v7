  "scripts": {
    "start": "egg-scripts start --daemon --title=wellapp-web",
    "stop": "egg-scripts stop --title=wellapp-web",
    "dev": "egg-bin dev --port=7001",
    "debug": "egg-bin debug --port=7001",
    "debug-web": "cross-env WELLAPP_WEBPACK_TARGET=web egg-bin debug --port=7001",
    "build": "cross-env easy clean && easy build prod",
    "build-zip": "cross-env easy clean && easy build prod && easy zip",
    "svg-sprite": "cross-env wellapp-svg-sprite",
    "wellapp-build": "cross-env wellapp-cli build",
    "wellapp-build-help": "cross-env wellapp-cli build --help",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "npm-link": "npm link ../wellapp-framework ../wellapp-page-assembly ../wellapp-admin  ../wellapp-dyform ../wellapp-workflow -f"
  },
  "ci": {
    "version": "10"
  },
  "author": "wellsoft",
  "lint-staged": {
    "../**/!(*.min).{html,js,css}": [
      "./node_modules/.bin/prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "files": [
    "app",
    ".babelrc",
    "bin",
    "config",
    "webpack.config.js",
    "favicon.png"
  ]
