<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <sql-query name="deleteHandoverItemByWorkHandoverUuid">
        <![CDATA[
		delete from WH_WORK_HANDOVER_ITEM
		where WH_WORK_HANDOVER_UUID = :workHandoverUuid
		
	]]>
    </sql-query>


</hibernate-mapping>