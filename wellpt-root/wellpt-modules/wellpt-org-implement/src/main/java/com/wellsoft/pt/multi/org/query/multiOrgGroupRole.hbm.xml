<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <!-- 删除指定角色的引用群组列表  -->
    <sql-query name="deleteGroupListOfRole">
        <![CDATA[
		delete from multi_org_group_role where role_uuid = :roleUuid
	]]>
    </sql-query>


</hibernate-mapping>