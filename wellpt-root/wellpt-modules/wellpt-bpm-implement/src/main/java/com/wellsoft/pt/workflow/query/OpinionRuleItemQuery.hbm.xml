<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <sql-query name="deleteByOpinionRuleUuid">
        <![CDATA[
		delete from WF_OPINION_RULE_ITEM
		where OPINION_RULE_UUID= :opinionRuleUuid
	]]>
    </sql-query>

    <sql-query name="getWfOpinionRuleItemListByOpinionRuleUuids">
        <![CDATA[
		select *
		from WF_OPINION_RULE_ITEM
		where OPINION_RULE_UUID in (:opinionRuleUuids)
	]]>
    </sql-query>


</hibernate-mapping>